{
  "name": "linter-cnew",
  "main": "./lib/main",
  "version": "0.1.1",
  "description": "Lint C and C++ source files using gcc or g++",
  "keywords": [
    "lint",
    "linter",
    "c",
    "c++",
    "gcc",
    "g++",
    "error"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zjcers/linter-cnew"
  },
  "license": "MIT",
  "engines": {
    "atom": ">1.0.0"
  },
  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  },
  "dependencies": {
    "atom-linter": "^8.0.0",
    "atom-package-deps": "^2.0.1",
    "shelljs": "^0.5.0"
  },
  "package-deps": [
    "linter"
  ],
  "devDependencies": {
    "eslint": "latest"
  },
  "eslintConfig": {
    "env": {
      "es6": true,
      "browser": true,
      "node": true
    },
    "ecmaFeatures": {
      "modules": true
    },
    "globals": {
      "atom": true
    }
  }
}
